<?xml version='1.0' encoding='utf-8'?>
<widget id="org.littletonrobotics.advantagescout" version="1.4.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Advantage Scout</name>
    <description>
        FRC Team 6328 custom scouting system
    </description>
    <author email="info@littletonrobotics.org" href="http://www.littletonrobotics.org">
        Littleton Robotics Scouting
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="www/img/logo256.png" width="256" height="256" density="mdpi" />
    <hook type="before_prepare" src="build_copy.sh" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
